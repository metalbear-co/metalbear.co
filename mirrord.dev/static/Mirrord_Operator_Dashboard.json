            "id": 556827580888684,
            "definition": {
                "title": "Sessions",
                "title_size": "16",
                "title_align": "left",
                "requests": [
                    {
                        "response_format": "event_list",
                        "query": {
                            "data_source": "logs_transaction_stream",
                            "query_string": "kube_deployment:mirrord-operator kube_container_name:mirrord-operator $Namespace $Target ",
                            "indexes": [],
                            "group_by": [
                                {
                                    "facet": "@fields.session_id"
                                }
                            ],
                            "compute": [
                                {
                                    "facet": "@fields.client_name",
                                    "aggregation": "most_frequent"
                                }
                            ],
                            "sort": {
                                "column": "timeline",
                                "order": "desc"
                            }
                        },
                        "columns": [
                            {
                                "field": "@fields.client_name:most_frequent",
                                "width": "auto"
                            },
                            {
                                "field": "group_by",
                                "width": "auto"
                            },
                            {
                                "field": "timeline",
                                "width": "auto"
                            },
                            {
                                "field": "duration",
                                "width": "auto"
                            }
                        ]
                    }
                ],
                "type": "list_stream"
            },
            "layout": {
                "x": 0,
                "y": 0,
                "width": 6,
                "height": 4
            }
        },
        {
            "id": 3267499080281826,
            "definition": {
                "title": "Targets",
                "title_size": "16",
                "title_align": "left",
                "time": {},
                "type": "query_table",
                "requests": [
                    {
                        "queries": [
                            {
                                "data_source": "logs",
                                "name": "query1",
                                "indexes": [
                                    "*"
                                ],
                                "compute": {
                                    "aggregation": "cardinality",
                                    "metric": "@fields.session_id"
                                },
                                "group_by": [
                                    {
                                        "facet": "@fields.target_namespace",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "cardinality",
                                            "metric": "@fields.session_id"
                                        }
                                    },
                                    {
                                        "facet": "@fields.target_type",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "cardinality",
                                            "metric": "@fields.session_id"
                                        }
                                    },
                                    {
                                        "facet": "@fields.target_name",
                                        "limit": 10,
                                        "sort": {
                                            "order": "desc",
                                            "aggregation": "cardinality",
                                            "metric": "@fields.session_id"
                                        }
                                    }
                                ],
                                "search": {
                                    "query": "kube_deployment:mirrord-operator kube_container_name:mirrord-operator $Namespace $Target"
                                },
                                "storage": "hot"
                            }
                        ],
                        "response_format": "scalar",
                        "formulas": [
                            {
                                "cell_display_mode": "bar",
                                "alias": "Sessions",
                                "formula": "query1",
                                "limit": {
                                    "count": 1000,
                                    "order": "desc"
                                }
                            }
                        ]
                    }
                ],
                "has_search_bar": "auto"
            },
            "layout": {
                "x": 6,
                "y": 0,
                "width": 6,
                "height": 4
            }
        },
